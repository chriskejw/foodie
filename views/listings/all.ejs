<%# ==============================================================%>
<ul id="all-listing-list">
<% allListings.forEach(function(listing) { %>
  <ol>
      <li>Listing:<%= listing.title %></li><br>
      <li>Listing:<%= listing.address %></li><br>
      <li>Listing:<%= listing.listingType %></li><br>
      <li>Listing:<%= listing.propertyType %></li><br>
      <li>Listing:<%= listing.price %><</li>br>
      <li>Listing:<%= listing.size %></li><br>
      <li>Listing:<%= listing.tenure %></li><br>
      <li>Listing:<%= listing.bedroom %></li><br>
      <li>Listing:<%= listing.bathroom %></li><br>
      <li>Listing:<%= listing.details %></li><br>
      <li>Listing:<%= listing.contactName %></li><br>
      <li>Listing:<%= listing.contactNumber %></li><br>
      <li>Listing:<%= ((listing.postDate).toLocaleDateString()) %></li><br>
  </ol>
<% }) %>
</ul>

<ul id="all-district-list">
<% allDistricts.forEach(function(district) { %>
  <li>
    <br><%= district.district %><br>
    <%= district.neighbourhood %><br>
  </li>
  <% }) %>
</ul>

